{"ast":null,"code":"import Hyperspeed from './components/Hyperspeed.vue';\nimport ClickSpark from './components/ClickSpark.vue';\nexport default {\n  name: 'App',\n  components: {\n    Hyperspeed,\n    ClickSpark\n  }\n};","map":{"version":3,"names":["Hyperspeed","ClickSpark","name","components"],"sources":["C:\\Users\\saine\\OneDrive\\Documents\\PresentAgent\\presentagent\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <Hyperspeed class=\"bg-animation\" />\r\n    <ClickSpark class=\"click-wrapper\">\r\n      <div class=\"content-overlay\">\r\n        <header class=\"app-header\">\r\n          <h1>PresentAgent</h1>\r\n        </header>\r\n        <main class=\"app-main\">\r\n          <router-view />\r\n        </main>\r\n      </div>\r\n    </ClickSpark>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Hyperspeed from './components/Hyperspeed.vue'\r\nimport ClickSpark from './components/ClickSpark.vue'\r\n\r\nexport default {\r\n  name: 'App',\r\n  components: {\r\n    Hyperspeed,\r\n    ClickSpark\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n/* Global reset for full screen */\r\nhtml, body {\r\n  margin: 0;\r\n  padding: 0;\r\n  height: 100%;\r\n  overflow: hidden; /* Hyperspeed handles overflow */\r\n}\r\n</style>\r\n\r\n<style scoped>\r\n#app {\r\n  position: relative;\r\n  width: 100vw;\r\n  height: 100vh;\r\n  overflow: hidden;\r\n  background-color: #000; /* Fallback for 3D */\r\n}\r\n\r\n/* Background Animation Layer */\r\n.bg-animation {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  z-index: 0;\r\n}\r\n\r\n/* Interaction Layer */\r\n.click-wrapper {\r\n  position: relative;\r\n  width: 100%;\r\n  height: 100%;\r\n  z-index: 1;\r\n}\r\n\r\n/* Content Container to center everything */\r\n.content-overlay {\r\n  position: relative;\r\n  display: flex;\r\n  flex-direction: column;\r\n  height: 100%;\r\n  pointer-events: none; /* Let clicks pass to ClickSpark mostly, re-enable for buttons */\r\n}\r\n\r\n.app-header {\r\n  font-family: 'American Typewriter', serif;\r\n  text-align: center;\r\n  font-weight: bold;\r\n  font-size: 24px;\r\n  height: 10vh;\r\n  color: #fff; /* White text for dark background */\r\n  text-shadow: 0 0 10px rgba(255,255,255,0.5);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  pointer-events: auto; /* Enable header interactions */\r\n  background: rgba(0,0,0,0.2); /* Slight dim */\r\n}\r\n\r\n.app-main {\r\n  flex: 1;\r\n  overflow: auto; /* Allow scrolling content */\r\n  font-family: \"Arial Rounded MT Bold\";\r\n  pointer-events: auto; /* Enable form interactions */\r\n  display: flex; /* Center router view */\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n</style>\r\n"],"mappings":"AAiBA,OAAOA,UAAS,MAAO,6BAA4B;AACnD,OAAOC,UAAS,MAAO,6BAA4B;AAEnD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVH,UAAU;IACVC;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}